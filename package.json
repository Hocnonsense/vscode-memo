{
  "name": "markdown-memo",
  "displayName": "Markdown Memo",
  "description": "Markdown knowledge base with first class bidirectional links",
  "publisher": "memo",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "markdown",
    "knowledge base",
    "references",
    "reference",
    "zettelkasten"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:memo.openRandomNote",
    "onCommand:memo.openTodayNote"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "commands": [
      {
        "command": "memo.openTodayNote",
        "title": "Open today's note",
        "category": "Memo"
      },
      {
        "command": "memo.openRandomNote",
        "title": "Open random note",
        "category": "Memo"
      }
    ]
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && rimraf ./out/test/workspace && ncp ./src/test/workspace ./out/test/workspace && rimraf ./out/test/workspace/.gitkeep",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.0",
    "@types/markdown-it": "^10.0.1",
    "@types/node": "^13.11.0",
    "@types/rimraf": "^3.0.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "ncp": "^2.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.8.3",
    "vscode-jest-test-runner": "^1.0.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "markdown-it": "^11.0.0",
    "markdown-it-regex": "^0.2.0"
  }
}
