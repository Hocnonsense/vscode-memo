{
	"name": "markdown-memo",
	"displayName": "Markdown Memo",
	"description": "Markdown knowledge base with first class bidirectional links",
	"publisher": "memo",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
    "markdown.markdownItPlugins": true
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/markdown-it": "^10.0.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"markdown-it": "^11.0.0",
		"markdown-it-regex": "^0.2.0"
	}
}
